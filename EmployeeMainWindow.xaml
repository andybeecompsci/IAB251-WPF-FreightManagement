<Window x:Class="IAB251_WPF_ASS2.EmployeeMainWindow"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:IAB251_WPF_ASS2"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EmployeeMainWindow">

    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Content="Register" Width="300" Height="40" Margin="0,10,0,0" Click="Register_Click"/>
            <Button Content="Login" Width="300" Height="40" Margin="0,10,0,0" Click="Login_Click"/>
            
        </StackPanel>
    </Grid>
</Window>
