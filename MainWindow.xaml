<Window x:Class="InterportCargo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Interport Cargo" Height="350" Width="525">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Content="Register" Width="200" Height="40" Margin="5" Click="Register_Click"/>
            <Button Content="Login" Width="200" Height="40" Margin="5" Click="Login_Click"/>
            <Button Content="Request Quotation" Width="200" Height="40" Margin="5" Click="RequestQuotation_Click"/>
        </StackPanel>
    </Grid>
</Window>
